{
  "name": "dega-themes",
  "license": "MIT",
  "private": true,
  "keywords": [
    "gatsby",
    "gatsby-theme",
    "gatsby-plugin",
    "factly"
  ],
  "bugs": {
    "url": "https://github.com/factly/dega-themes/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/factly/dega-themes.git"
  },
  "workspaces": [
    "themes/*",
    "examples/*",
    "plugins/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check \"**/*.{js,jsx,yml,yaml,json,css,scss,md}\"",
    "format": "prettier --write \"**/*.{js,jsx,yml,yaml,json,css,scss,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format"
    }
  },
  "dependencies": {
    "gatsby": "^2.21.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.3.0",
    "husky": "^4.3.0",
    "lerna": "^3.20.2",
    "prettier": "^2.1.2"
  }
}
